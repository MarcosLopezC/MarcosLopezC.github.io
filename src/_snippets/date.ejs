<%-
	(function() {
		if (!date) {
			return "";
		}

		date = new Date(date.replace(/-/g, "/"));

		const month = [
			"January",
			"February",
			"March",
			"April",
			"May",
			"June",
			"July",
			"August",
			"September",
			"October",
			"November",
			"December"
		];

		return `${month[date.getMonth()]} ${date.getDate()}, ${date.getFullYear()}`;
	}());
_%>